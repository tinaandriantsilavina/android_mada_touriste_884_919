<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="progressBarColor" parent="@style/Theme.AppCompat">
        <item name="colorAccent">@color/security</item>
    </style>
    <style name="fullViewTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/secondary</item>
        <item name="colorAccent">@color/black</item>
    </style>

    <style name="Circular">
        <item name="cornerSize">50%</item>

    </style>

    <style name="BottomNavigation">
        <item name="android:background">@drawable/shadow</item>
        <item name="backgroundTint">@null</item>
        <item name="elevation">@dimen/m3_sys_elevation_level2</item>
        <item name="itemTextAppearanceInactive">?attr/textAppearanceCaption</item>
        <item name="itemTextAppearanceActive">?attr/textAppearanceCaption</item>
        <item name="itemRippleColor">@color/grey</item>
        <item name="itemIconTint">@color/vert</item>
        <item name="itemTextColor">@color/countBackgroundColor</item>
        <item name="itemPaddingTop">5dp</item>
        <item name="itemPaddingBottom">@dimen/m3_bottom_nav_item_padding_bottom</item>
        <item name="android:minHeight">@dimen/m3_bottom_nav_min_height</item>
        <item name="enforceTextAppearance">true</item>
        <item name="enforceMaterialTheme">true</item>
        <item name="labelVisibilityMode">unlabeled</item>
    </style>
</resources>